{
  "manifest_version": 3,
  "name": "Tradutor MSA Extensão",
  "version": "2.1",
  "description": "Extensão de tradução simples e rápida.",
  "icons": {
    "16": "440x280_Tradutor.png",
    "48": "440x280_Tradutor.png",
    "128": "440x280_Tradutor.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Traduzir"
  },
  "background": {
    "service_worker": "background.js"
  }
}